<h3>HOME</h3>

<section class="allProducts">
<table>
    <thead>
        <tr>
             <th>ID</th>
            <th>Title</th>
            <th>Description</th>
            <th>Price</th>
            <th>Thumbnail</th>
            <th>Code</th>
            <th>Stock</th>
            <th>Status</th>
            <th>Category</th>
        </tr>
    </thead>
    <tbody>
        {{#each products}}
        <tr>
             <td>{{id}}</td>
            <td>{{title}}</td>
            <td>{{description}}</td>
            <td>$ {{price}}</td>
            <td>{{thumbnail}}</td>
            <td> {{code}} </td>
            <td> {{stock}} </td>
            <td> {{status}} </td>
            <td> {{category}} </td>
        </tr>
        {{/each}}
    </tbody>
</table>
</section>
